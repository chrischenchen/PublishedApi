[
  {
    "author": "韩绛", 
    "paragraphs": [
      "嵩峤云高，洛川波暖。", 
      "举头乔木森无断。", 
      "□□□雨绝风尘，小桥频过春渠满。", 
      "□□离宫，□棱斗焕。", 
      "万家罗绮多游伴。", 
      "□□□□自风□，□□是处喧弦管。"
    ], 
    "rhythmic": "踏莎行"
  }, 
  {
    "author": "李师中", 
    "paragraphs": [
      "子规啼破城楼月。", 
      "画船晓载笙歌发。", 
      "两岸荔枝红。", 
      "万家烟雨中。", 
      "佳人相对泣。", 
      "泪下罗衣湿。", 
      "从此信音稀。", 
      "岭南无雁飞。"
    ], 
    "rhythmic": "菩萨蛮"
  }, 
  {
    "author": "蔡挺", 
    "paragraphs": [
      "霜天清晓。", 
      "望紫塞古垒，寒云衰草。", 
      "汗马嘶风，边鸿翻月，垅上铁衣寒早。", 
      "剑歌骑曲悲壮，尽道君恩难报。", 
      "塞垣乐，尽双鞬锦带，山西年少。", 
      "谈笑。", 
      "刁斗静。", 
      "烽火一把，常送平安耗。", 
      "圣主忧边，威灵遐布，骄虏且宽天讨。", 
      "岁华向晚愁思，谁念玉关人老。", 
      "太平也，且欢娱，不惜金尊频倒。"
    ], 
    "rhythmic": "喜迁莺"
  }, 
  {
    "author": "王益柔", 
    "paragraphs": [
      "秋云朔吹晓徘徊。", 
      "雪照楼台。", 
      "梁王宴召有邹枚。", 
      "相如独逞雄才。", 
      "明烛薰炉香暖，深劝金杯。", 
      "庭前粉艳有寒梅。", 
      "一枝昨夜先开。"
    ], 
    "rhythmic": "喜长新"
  }, 
  {
    "author": "韩维", 
    "paragraphs": [
      "早岁相期林下，高年同在尊前。", 
      "风花绣舞乍晴天。", 
      "绿蚁新浮酒面。", 
      "身外虚名电转，人间急景梭传。", 
      "当筵莫惜听朱弦。", 
      "一品归来强健。"
    ], 
    "rhythmic": "西江月"
  }, 
  {
    "author": "韩维", 
    "paragraphs": [
      "归雁低空，游蜂趁暖。", 
      "凭高目向西云断。", 
      "具茨山外夕阳多，展江亭下春波满。", 
      "双桂情深，千花明焕。", 
      "良辰谁是同游伴。", 
      "辛夷花谢早梅开。", 
      "应须次第调弦管。"
    ], 
    "rhythmic": "踏莎行"
  }, 
  {
    "author": "韩维", 
    "paragraphs": [
      "和风动□。", 
      "□□新年□入手。", 
      "世事尘□。", 
      "□□□情近酒□。", 
      "水开湖□。", 
      "□□笙歌波面起。", 
      "相与排□。", 
      "□□□胜特地□。"
    ], 
    "rhythmic": "减字木兰花"
  }, 
  {
    "author": "韩维", 
    "paragraphs": [
      "饱食日□□。", 
      "□上危亭。", 
      "东风昨夜入□□。", 
      "□□雪晴云□□，□遍银屏。", 
      "回首叹劳生。", 
      "□鼎相承。", 
      "皇恩早晚□□□。", 
      "□□陂边垂钓手，不负幽情。"
    ], 
    "rhythmic": "浪淘沙"
  }, 
  {
    "author": "韩维", 
    "paragraphs": [
      "夜来风横雨飞狂，满地闲花衰草。", 
      "燕子渐归春悄。", 
      "帘幕垂清晓。", 
      "天将佳景与闲人，美酒宁嫌华皓。", 
      "留取旧时欢笑。", 
      "莫共秋光老。"
    ], 
    "rhythmic": "胡捣练令・胡捣练"
  }, 
  {
    "author": "韩维", 
    "paragraphs": [
      "轻云薄雾。", 
      "散作催花雨。"
    ], 
    "rhythmic": "失调名"
  }, 
  {
    "author": "韩维", 
    "paragraphs": [
      "兄弟对举杯。"
    ], 
    "rhythmic": "失调名"
  }, 
  {
    "author": "曾巩", 
    "paragraphs": [
      "暮冬天地闭，正柔木冻折，瑞雪飘飞。", 
      "对景见南山，岭梅露、几点清雅容姿。", 
      "丹染萼、玉缀枝。", 
      "又岂是、一阳有私。", 
      "大抵是、化工独许，使占却先时。", 
      "霜威莫苦凌持。", 
      "此花根性，想群卉争知。", 
      "贵用在和羹，三春里、不管绿是红非。", 
      "攀赏处、宜酒卮。", 
      "醉拈嗅、幽香更奇。", 
      "倚阑干、仗何人去，嘱羌管休吹。"
    ], 
    "rhythmic": "赏南枝"
  }, 
  {
    "author": "司马光", 
    "paragraphs": [
      "渔舟容易入春山。", 
      "仙家日月闲。", 
      "绮窗纱幌映朱颜。", 
      "相逢醉梦间。", 
      "松露冷，海霞殷。", 
      "匆匆整棹还。", 
      "落花寂寂水潺潺。", 
      "重寻此路难。"
    ], 
    "rhythmic": "阮郎归"
  }, 
  {
    "author": "司马光", 
    "paragraphs": [
      "宝髻松松挽就，铅华淡淡妆成。", 
      "青烟翠雾罩轻盈。", 
      "飞絮游丝无定。", 
      "相见争如不见，有情何似无情。", 
      "笙歌散后酒初醒。", 
      "深院月斜人静。"
    ], 
    "rhythmic": "西江月"
  }, 
  {
    "author": "司马光", 
    "paragraphs": [
      "红日迟迟，虚廊转影，槐阴迤逦西斜。", 
      "彩笔工夫，难状晚景烟霞。", 
      "蝶尚不知春去，漫绕幽砌寻花。", 
      "奈猛风过后，纵有残红，飞向谁家。", 
      "始知青鬓无价，叹飘零官路，荏苒年华。", 
      "今日笙歌丛里，特地咨嗟。", 
      "席上青衫湿透，算感旧、何止琵琶。", 
      "怎不教人易老，多少离愁，散在天涯。"
    ], 
    "rhythmic": "锦堂春・锦堂春慢"
  }, 
  {
    "author": "苏氏", 
    "paragraphs": [
      "一夜东风穿绣户，融融暖应佳时。", 
      "春来何处最先知。", 
      "平明堤上柳。", 
      "染遍郁金枝。", 
      "姊妹嬉游时节近，今朝应怨来迟。", 
      "凭谁说与到家期。", 
      "玉钗头上胜，留待远人归。"
    ], 
    "rhythmic": "临江仙"
  }, 
  {
    "author": "苏氏", 
    "paragraphs": [
      "小阑干，深院宇。", 
      "依旧当时别处。", 
      "朱户锁，玉楼空。", 
      "一帘霜日红。", 
      "弄珠江，何处是，望断碧云无际。", 
      "凝泪眼，出重城。", 
      "隔溪羌笛声。"
    ], 
    "rhythmic": "更漏子"
  }, 
  {
    "author": "苏氏", 
    "paragraphs": [
      "星移斗转，玉蟾西下，渐觉东郊向晓。", 
      "马嘶人语隔霜林，望千里、长安古道。", 
      "珠宫姊妹，相逢方信，别后十分瘦了。", 
      "上林归去正花时，争奈向、花前又老。"
    ], 
    "rhythmic": "鹊桥仙"
  }, 
  {
    "author": "苏氏", 
    "paragraphs": [
      "孤馆深沈，晓寒天气。", 
      "解鞍独自阑干倚。", 
      "暗香浮动月黄昏，落梅风送沾衣袂。", 
      "待写红笺，凭谁与寄。", 
      "先教觅取嬉游地。", 
      "到家正是早春时，小桃花下拼沈醉。"
    ], 
    "rhythmic": "踏莎行"
  }, 
  {
    "author": "刘敞", 
    "paragraphs": [
      "小山丛桂。", 
      "最有留人意。", 
      "拂叶攀花无限思。", 
      "雨湿浓香满袂。", 
      "别来过了秋光。", 
      "翠帘昨夜新霜。", 
      "多少月宫闲地，姮娥与借微芳。"
    ], 
    "rhythmic": "清平乐"
  }, 
  {
    "author": "刘敞", 
    "paragraphs": [
      "蜡炬高高，龙烟细细。", 
      "玉楼十二门初闭。", 
      "疏帘不卷水晶寒，小屏半掩琉璃翠。", 
      "桃叶新声，榴花美味。", 
      "南山宾客东山妓。", 
      "利名不肯放人闲，忙中偷取工夫醉。"
    ], 
    "rhythmic": "踏莎行"
  }, 
  {
    "author": "王", 
    "paragraphs": [
      "忆玉清景，繁盛极当时。", 
      "千古事难追。", 
      "汉家别庙秋风起，空出奉宸衣。", 
      "三山浮海日晖晖。", 
      "羽盖共云飞。", 
      "灵宫旧是栖真处，还望玉舆归。"
    ], 
    "rhythmic": "导引"
  }, 
  {
    "author": "王", 
    "paragraphs": [
      "玉宸朝晚，忽掩赭黄衣。", 
      "愁雾锁金扉。", 
      "蓬莱待得仙丹至，人世已成非。", 
      "龙轩天仗转西畿。", 
      "旌旆入云飞。", 
      "望陵宫女垂红泪，不见翠舆归。"
    ], 
    "rhythmic": "发引"
  }, 
  {
    "author": "王", 
    "paragraphs": [
      "上林春晚，曾是奉宸游。", 
      "水殿戏龙舟。", 
      "玉箫吹断催仙驭，一去隔千秋。", 
      "游人重到曲江头。", 
      "事往涕难收。", 
      "空馀御幄传觞处，依旧水东流。"
    ], 
    "rhythmic": "发引"
  }, 
  {
    "author": "韩缜", 
    "paragraphs": [
      "锁离愁，连绵无际，来时陌上初熏。", 
      "绣帏人念远，暗垂珠泪，泣送征轮。", 
      "长亭长在眼，更重重、远水孤云。", 
      "但望极楼高，尽日目断王孙。", 
      "消魂。", 
      "池塘别后，曾行处、绿妒轻裙。", 
      "恁时携素手，乱花飞絮里，缓步香裀。", 
      "朱颜空自改，向年年、芳意长新。", 
      "遍绿野，嬉游醉眠，莫负青春。"
    ], 
    "rhythmic": "凤箫吟・芳草", 
    "tags": [
      "宋词三百首"
    ]
  }, 
  {
    "author": "韩缜姬", 
    "paragraphs": [
      "香作风光浓著露。", 
      "正恁双栖，又遣分飞去。", 
      "密诉东君应不许。", 
      "泪波一洒奴衷素。"
    ], 
    "rhythmic": "蝶恋花"
  }, 
  {
    "author": "裴湘", 
    "paragraphs": [
      "雁塞说并门。", 
      "郡枕西汾。", 
      "山形高下远相吞。", 
      "古寺楼台依碧嶂，烟景遥分。", 
      "晋庙锁溪云。", 
      "箫鼓仍存。", 
      "牛羊斜日自归村。", 
      "惟有故城禾黍地，前事消魂。"
    ], 
    "rhythmic": "浪淘沙"
  }, 
  {
    "author": "裴湘", 
    "paragraphs": [
      "万国仰神京。", 
      "礼乐纵横。", 
      "葱葱佳气锁龙城。", 
      "日御明堂天子圣，朝会簪缨。", 
      "九陌六街平。", 
      "万国充盈。", 
      "青楼弦管酒如渑。", 
      "别有隋堤烟柳暮，千古含情。"
    ], 
    "rhythmic": "浪淘沙"
  }, 
  {
    "author": "阮逸女", 
    "paragraphs": [
      "仙苑春浓，小桃开，枝枝已堪攀折。", 
      "乍雨乍晴，轻暖轻寒，渐近赏花时节。", 
      "柳摇台榭东风软，帘栊静、幽禽调舌。", 
      "断魂远、闲寻翠径，顿成愁结。", 
      "此恨无人共说。", 
      "还立尽黄昏，寸心空切。", 
      "强整绣衾，独掩朱扉，簟枕为谁铺设。", 
      "夜长更漏传声远，纱窗映、银缸明灭。", 
      "梦回处，梅梢半笼淡月。"
    ], 
    "rhythmic": "花心动"
  }, 
  {
    "author": "滕甫", 
    "paragraphs": [
      "叶底无风池面静。", 
      "掬水佳人，拍破青铜镜。", 
      "残月朦胧花弄影。", 
      "新梳斜插乌云鬓。", 
      "拍索闷怀添酒兴。", 
      "旋撷园蔬，随分成盘饤。", 
      "说与翠微休急性。", 
      "功名富贵皆前定。"
    ], 
    "rhythmic": "蝶恋花"
  }, 
  {
    "author": "滕甫", 
    "paragraphs": [
      "昼永无人深院静。", 
      "一枕春醒，犹未忺临镜。", 
      "帘卷新蟾光射影。", 
      "速忙掠起蓬松鬓。", 
      "对景沈吟嗟没兴。", 
      "薄幸不来，空把杯盘饤。", 
      "休道妇人多水性。", 
      "今宵独自言无定。"
    ], 
    "rhythmic": "蝶恋花"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "登临送目。", 
      "正故国晚秋，天气初肃。", 
      "千里澄江似练，翠峰如簇。", 
      "归帆去棹残阳里，背西风、酒旗斜矗。", 
      "彩舟云淡，星河鹭起，画图难足。", 
      "念往昔、繁华竞逐。", 
      "叹门外楼头，悲恨相续。", 
      "千古凭高，对此漫嗟荣辱。", 
      "六朝旧事随流水，但寒烟、芳草凝绿。", 
      "至今商女，时时犹唱，后庭遗曲。"
    ], 
    "rhythmic": "桂枝香", 
    "tags": [
      "宋词三百首"
    ]
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "折得一枝香在手。", 
      "人间应未有。", 
      "疑是经春雪未消。", 
      "今日是何朝。"
    ], 
    "rhythmic": "甘露歌"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "尽日含毫难比兴。", 
      "都无色可并。", 
      "万里晴天何处来。", 
      "真是屑琼瑰。"
    ], 
    "rhythmic": "甘露歌"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "天寒日暮山谷里。", 
      "的砾愁成水。", 
      "池上渐多枝上稀。", 
      "唯有故人知。"
    ], 
    "rhythmic": "甘露歌"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "数家茅屋闲临水。", 
      "单衫短帽垂杨里。", 
      "今日是何朝。", 
      "看予度石桥。", 
      "梢梢新月偃。", 
      "午醉醒来晚。", 
      "何物最关情。", 
      "黄鹂三两声。"
    ], 
    "rhythmic": "菩萨蛮"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "灯火已收正月半。", 
      "山南山北花撩乱。", 
      "闻说洊亭新水漫。", 
      "骑款段。", 
      "穿云入坞寻游伴。", 
      "却拂僧床褰素幔。", 
      "千岩万壑春风暖。", 
      "一弄松声悲急管。", 
      "吹梦断。", 
      "西看窗日犹嫌短。"
    ], 
    "rhythmic": "渔家傲"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "平岸小桥千嶂抱。", 
      "柔蓝一水萦花草。", 
      "茅屋数间窗窈窕。", 
      "尘不到。", 
      "时时自有春风扫。", 
      "午枕觉来闻语鸟。", 
      "敧眠似听朝鸡早。", 
      "忽忆故人今总老。", 
      "贪梦好。", 
      "茫然忘了邯郸道。"
    ], 
    "rhythmic": "渔家傲"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "孜孜矻矻。", 
      "向无明里、强作窠窟。", 
      "浮名浮利何济，堪留恋处，轮回仓猝。", 
      "幸有明空妙觉，可弹指超出。", 
      "缘底事、抛了全潮，认一浮沤作瀛渤。", 
      "本源自性天真佛。", 
      "祗些些、妄想中埋没。", 
      "贪他眼花阳艳，谁信道、本来无物。", 
      "一旦茫然，终被阎罗老子相屈。", 
      "便纵有、千种机筹，怎免伊唐突。"
    ], 
    "rhythmic": "雨霖铃"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "云垂平野。", 
      "掩映竹篱茅舍。", 
      "阒寂幽居实潇洒。", 
      "是处绿娇红冶。", 
      "丈夫运用堂堂。", 
      "且莫五角六张。", 
      "若有一卮芳酒，逍遥自在无妨。"
    ], 
    "rhythmic": "清平乐"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "百亩中庭半是苔。", 
      "门前白道水萦回。", 
      "爱闲能有几人来。", 
      "小院回廊春寂寂，山桃溪杏两三栽。", 
      "为谁零落为谁开。"
    ], 
    "rhythmic": "浣溪沙"
  }, 
  {
    "author": "王安石", 
    "paragraphs": [
      "常时黄色见眉间。", 
      "松桂我同攀。", 
      "每言天上辛苦，不肯饵金丹。", 
      "怜水静，爱云闲。", 
      "便忘还。", 
